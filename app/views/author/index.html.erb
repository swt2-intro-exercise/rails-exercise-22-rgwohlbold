<%= link_to 'New', new_author_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Homepage</th>
  </tr>
  <% @authors.each do |author| %>
    <tr>
      <td><%= author.name %></td>
      <td><%= link_to author.homepage, author.homepage %></td>
      <td><%= link_to 'View', author_path(author) %></td>
      <td><%= link_to 'Edit', edit_author_path(author) %></td>
    </tr>
    <% end %>
</table>
